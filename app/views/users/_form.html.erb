<%= form_for @user do |f| %>
<%= render 'errors' %>
  
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email, placeholder: "Email" %>
  </p>

 
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password, placeholder: "Password" %>
  </p>

  


  <p>
    <%= f.submit "Submit", class: "btn-lg btn-primary"%>
  </p>

<% end %>